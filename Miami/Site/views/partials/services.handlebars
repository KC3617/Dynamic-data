<div class="row">
<div class="accordion" id="servicesAccordion">
    {{#each data.services}}
  <div class="accordion-item">
    <h2 class="accordion-header">
      <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#collapse_{{@index}}" aria-expanded="true" aria-controls="collapse_{{@index}}">
        {{this.title}}
      </button>
    </h2>
    <div id="collapse_{{@index}}" class="accordion-collapse collapse show" data-bs-parent="servicesAccordion">
      <div class="accordion-body">
        <p>{{this.intro}}</p>
        <ul>
            {{#each this.list}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
        {{#if this.image}}
        <img class="img-fluid" src="{{this.image}}" alt="">
        {{/if}}
    </div>
    </div>
  </div>
    {{/each}}
  </div>
</div>